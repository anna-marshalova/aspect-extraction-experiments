{
  "mbert": {
    "transformer_model": "TFBertForTokenClassification"},
  "rubert": {
    "pretrained_model_name": "DeepPavlov/rubert-base-cased",
    "transformer_model": "TFBertForTokenClassification",
    "from_pt": true},
  "rubert-tiny": {
    "pretrained_model_name": "cointegrated/rubert-tiny2",
    "transformer_model": "TFBertForTokenClassification",
    "from_pt": true
  },
  "ruSciBERT": {
    "pretrained_model_name": "sberbank-ai/ruSciBERT",
    "transformer_config": "RobertaConfig",
    "transformer_model": "TFRobertaForTokenClassification",
    "transformer_tokenizer": "RobertaTokenizer",
    "from_pt": true
  },
  "mbert_bilstm": {"bilstm_size": 50},
  "mbert_bilstm_frozen": {"bilstm_size": 50,"freeze_weights": true},
  "mbert_crf": {
    "add_crf": true,
    "loss" : "SigmoidFocalCrossEntropy"
  },
  "mbert_for_sents": {"classify_sequence": true}
}
